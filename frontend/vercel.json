{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "functions": {},
  "outputDirectory": "public",
  "buildCommand": "echo 'Static site - no build needed'",
  "installCommand": "npm install",
  "framework": null
} 